    "maintenance:update": "node scripts/maintenance.js --update",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist dev-dist coverage .vite node_modules/.cache",
    "clean:deep": "rm -rf dist dev-dist coverage .vite node_modules backend/node_modules && npm install"
